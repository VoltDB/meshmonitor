  "widgets": [
    {
      "id": 3196381106031784,
      "definition": {
        "title": "Worst ping time across cluster",
        "title_size": "16",
        "title_align": "left",
        "type": "query_value",
        "requests": [
          {
            "response_format": "scalar",
            "queries": [
              {
                "name": "query1",
                "data_source": "metrics",
                "query": "max:meshmonitor.meshmonitor_receive_seconds{*}",
                "aggregator": "avg"
              }
            ],
            "conditional_formats": [
              {
                "comparator": ">",
                "value": 0.25,
                "palette": "white_on_red"
              }
            ],
            "formulas": [
              {
                "formula": "query1"
              }
            ]
          }
        ],
        "autoscale": false,
        "custom_unit": "s",
        "precision": 3,
        "timeseries_background": {
          "yaxis": {
            "include_zero": false
          },
          "type": "area"
        }
      },
      "layout": {
        "x": 0,
        "y": 0,
        "width": 4,
        "height": 2
      }
    },
